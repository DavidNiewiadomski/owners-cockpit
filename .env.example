# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# Document Processing
ADOBE_PDF_CLIENT_ID=your-adobe-pdf-client-id
ADOBE_PDF_CLIENT_SECRET=your-adobe-pdf-client-secret
ADOBE_PDF_ORGANIZATION_ID=your-adobe-organization-id
GOOGLE_CLOUD_VISION_API_KEY=your-google-vision-api-key

# Microsoft Office 365 Integration
OFFICE365_CLIENT_ID=your-office365-client-id
OFFICE365_CLIENT_SECRET=your-office365-client-secret
OFFICE365_TENANT_ID=your-office365-tenant-id

# Procore Integration
PROCORE_CLIENT_ID=your-procore-client-id
PROCORE_CLIENT_SECRET=your-procore-client-secret

# Application Configuration
VITE_APP_URL=http://localhost:3000
NODE_ENV=development

# AI Proxy Configuration (Local Development)
VITE_AI_PROXY_URL=http://localhost:3001
# Set to true to force using Supabase edge functions in development
# VITE_FORCE_SUPABASE=true

# Security Configuration
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here
SESSION_SECRET=your-session-secret-here

# Database Configuration
SUPABASE_DB_URL=your-database-url-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
