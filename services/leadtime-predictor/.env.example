# Lead Time Predictor Service Environment Variables

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# ENR Data API (Optional - uses mock data if not provided)
ENR_API_KEY=your-enr-api-key-here

# Model Configuration (Optional)
MODEL_RETRAIN_SCHEDULE="0 2 * * *"  # Daily at 2 AM
MODEL_MIN_TRAINING_RECORDS=50
MODEL_CONFIDENCE_THRESHOLD=0.8

# Service Configuration (Optional)
LOG_LEVEL=INFO
DEBUG_MODE=false
MAX_WORKERS=4
